    [
      [
        "%m,axh002"
      ],
      [
        "%g,black"
      ],
      [
        "%s,24"
      ],
      [
        "――ドタドタ",
        "%k",
        "【浩　次】なんで、オレが起こしにいかなきゃ……。",
        "%k",
        "浩次は、ぶつぶつ言いながらも、階段を上る。",
        "%k",
        "なぜ、姉を起こすのがイヤなのか？",
        "%k",
        "愛娘を起こしに行こうとしない母。",
        "%k",
        "息子に無言で押しつける父。",
        "%k",
        "そして、今朝も向かう浩次――",
        "%k",
        "それは――",
        "%k",
        "美弥子の寝起きは――",
        "%k",
        "――ガチャ",
        "%k",
        "%g,cg12"
      ],
      [
        "【浩　次】あねき～。起きて……。",
        "%k",
        "浩次はため息をついた。",
        "%k",
        "【浩　次】――るわけねーよなぁ……。",
        "%k",
        "淡い期待をしたが、無駄だった。",
        "%k",
        "心地よさそうに夢の世界の住人になっている。",
        "%k",
        "さーてと、ばかりにため息をつくと、どうしようかと逡巡する。",
        "%k",
        "【浩　次】おきろ～～～。おきろ、おきろ、おきれって！",
        "%k",
        "何度も声をかける。",
        "%k",
        "体を揺さぶる。",
        "%k",
        "肩を持ってガクンガクンと揺さぶった――",
        "%k",
        "%v,b04am001"
      ],
      [
        "【美弥子】ん～～ぐぅう、すぅうう、ぐぅううう～～。",
        "%k",
        "%v,0"
      ],
      [
        "が、当然起きるはずもなく――",
        "%k",
        "心地よさそうに寝こけている。",
        "%k",
        "寝付きがいいくせに、寝起きが悪い。",
        "%k",
        "しかも起こしてもテコでも起きない。",
        "%k",
        "ある意味、最悪だ。",
        "%k",
        "【浩　次】おきろって……ばよ。ご飯冷めるぞ～。",
        "%k",
        "食い意地が張ってるはずなのに、この手も通じない。",
        "%k",
        "【浩　次】乳もんだろか、どさくさに紛れて――",
        "%k",
        "浩次は大きくため息を漏らすと、顔を向ける。",
        "%k",
        "思わず鼻息が荒くなる。",
        "%k",
        "そして、手を伸ばしかける――",
        "%k",
        "そんな、空気を読んだのか――",
        "%k",
        "%v,b04am002"
      ],
      [
        "【美弥子】ん～～～、なぁにぃい～～～。",
        "%k",
        "%v,0"
      ],
      [
        "%g,cg12a"
      ],
      [
        "目をゴシゴシと、こすりながら、顔を上げた。",
        "%k",
        "薄目で、浩次を見る美弥子。",
        "%k",
        "【浩　次】（ばれた！）",
        "%k",
        "思わず、伸ばしかけた手を慌てて引く。",
        "%k",
        "その瞬間、姉の目がキュピーン！と光った。",
        "%k",
        "【浩　次】（見られた！）",
        "%k",
        "%v,b04am003"
      ],
      [
        "【美弥子】あんた……なにしてんの……。",
        "%k",
        "%v,0"
      ],
      [
        "そう言いながら、ぐっと体を起こす。",
        "%k",
        "【浩　次】あっ、いや……それは、その……。",
        "%k",
        "%v,b04am004"
      ],
      [
        "【美弥子】なにしてんのって聞いてるの！",
        "%k",
        "%v,0"
      ],
      [
        "――ドターン！",
        "%k",
        "足払いと当時に床に倒される。",
        "%k",
        "【浩　次】いってぇ！",
        "%k",
        "%v,b04am005"
      ],
      [
        "【美弥子】人の安眠じゃまして、なにしてるのかいいなさい！",
        "%k",
        "%v,0"
      ],
      [
        "【浩　次】起こしにきたのに、それはねーだろぉお！",
        "%k",
        "%g,cg13"
      ],
      [
        "倒れたところに――",
        "%k",
        "%v,b04am006"
      ],
      [
        "【美弥子】嘘おっしゃい！",
        "%k",
        "%v,0"
      ],
      [
        "【浩　次】うぐぅう！",
        "%k",
        "顔を挟まれる。",
        "%k",
        "ぎりぎりと、全力で締め上げられる。",
        "%k",
        "足の力は腕の倍以上――",
        "%k",
        "耐えられるはずもなく――",
        "%k",
        "【浩　次】ぐ、ぐるじ……。",
        "%k",
        "しかし、顔面には姉の股間。",
        "%k",
        "柔らかい土手の感触が顔全体で――",
        "%k",
        "%v,b04am007"
      ],
      [
        "【美弥子】あんた、いま、なんかしようとしてなかった！",
        "%k",
        "%v,0"
      ],
      [
        "やっぱりばれてた苦悶式！",
        "%k",
        "確かに、姉の寝起きは最悪だが、このような直接攻撃は、珍しく……ないか……。",
        "%k",
        "ぎりぎりと締め上げられる。",
        "%k",
        "苦しい。",
        "%k",
        "けど、うれしい。",
        "%k",
        "うれしい。",
        "%k",
        "けど、ぐ、ぐるぢぃ……。",
        "%k",
        "%v,b04am008"
      ],
      [
        "【美弥子】ほら！ギブ？ギブ？",
        "%k",
        "%v,0"
      ],
      [
        "【浩　次】おっ、ぐっ……。",
        "%k",
        "足にどんどん力がこもる。",
        "%k",
        "顔が、食い込んでゆく。",
        "%k",
        "姉の香り。",
        "%k",
        "息を吸い込む度に鼻孔を刺激する。",
        "%k",
        "うれしい……けど……。",
        "%k",
        "楽しむよゆうもな……。",
        "%k",
        "【浩　次】ぐ、ぐるじ……。",
        "%k",
        "ジタジタと、全身でもがいているが、姉の動きは止まらない。",
        "%k",
        "%v,b04am009"
      ],
      [
        "【美弥子】あたしの寝起きを襲おうなんて、百年早い！わかった！",
        "%k",
        "%v,0"
      ],
      [
        "ぐい！ぐぐぐぐぐぐぐっ、ぐいい！",
        "%k",
        "【浩　次】おっ、ぐっ……ぐる、ぢ……。",
        "%k",
        "%v,b04am010"
      ],
      [
        "【美弥子】わかったって聞いてるの！",
        "%k",
        "%v,0"
      ],
      [
        "美弥子は半分楽しんでいるのかもしれない。",
        "%k",
        "でもなければ、ギヴ！と訪ねたりもしない。",
        "%k",
        "だが――、さすがに、返事もしにくいことも気がついているのだろう。",
        "%k",
        "助け船を出すあたり、やはり、それなりに気を使ってはいる。",
        "%k",
        "のだけども……。",
        "%k",
        "りきききっ！足に力がこもる。",
        "%k",
        "容赦していない。",
        "%k",
        "しかし、気持ちいい――",
        "%k",
        "%v,b04am011"
      ],
      [
        "【美弥子】ギブ？ギヴ？",
        "%k",
        "%v,0"
      ],
      [
        "ごりり……。",
        "%k",
        "目の前が真っ白になってくる……。",
        "%k",
        "なぜか聞こえるタイムカウント――",
        "%k",
        "１０、",
        "%k",
        "９、",
        "%k",
        "８、",
        "%k",
        "７、",
        "%k",
        "６、",
        "%k",
        "５、",
        "%k",
        "４、",
        "%k",
        "３、",
        "%k",
        "２、",
        "%k",
        "１、",
        "%k",
        "一つカウントダウンする度にマッチョなダンディーのポージングが脳内で再生される。",
        "%k",
        "０、",
        "%k",
        "ゼロに届いたとき――",
        "%k",
        "【浩　次】（あっ、あにきぃいい～～もっ、もう……だめだぁあ！）",
        "%k",
        "――ちゅどーん！",
        "%k",
        "頭の中で、誰かの叫び声が聞こえ――",
        "%k",
        "パタリ……。",
        "%k",
        "%g,cg13a"
      ],
      [
        "必死にもがいていた、浩次の腕から力が抜ける。",
        "%k",
        "顔も真っ青だ。",
        "%k",
        "さすがに、美弥子もこの異変に気がついて――",
        "%k",
        "%v,b04am012"
      ],
      [
        "【美弥子】んっ？ちょ！ちょっと！あんた、なんで――",
        "%k",
        "%v,0"
      ],
      [
        "慌てながら声を上げる。",
        "%k",
        "【浩　次】…………。",
        "%k",
        "%v,b04am013"
      ],
      [
        "【美弥子】ギブしなさいって言ったでしょ！",
        "%k",
        "%v,0"
      ],
      [
        "ガクガクと全身を揺さぶられながら、ゆっくり息をする。",
        "%k",
        "%v,b04am014"
      ],
      [
        "【美弥子】ちょ、ちょっとお！息しなさいよ！",
        "%k",
        "%v,0"
      ],
      [
        "だから、我が家では誰も姉を起こしたがらないのだ――",
        "%k",
        "だが、しかし――",
        "%k",
        "土手の感触を楽しむために、わざと苦悶したというのは、誰にも言わない、君と僕との秘密だ！",
        "%k",
        "乳を揉めないのなら、せめて、これぐらいの役得はほしい。",
        "%k",
        "まさに、漢と書いてオトコと呼ぶ、真の漢の生き様であった！",
        "%k",
        "%g,black"
      ],
      [
        "――チーン",
        "%k",
        "南無……阿弥陀仏！",
        "%k",
        ""
      ]
